import java.util.Scanner;

class fib{
	public static void main(String args[]){
		Scanner obj=new Scanner(System.in);
		System.out.print("Enter how many fibonacci number you want:");
		int n = obj.nextInt();
		int n1=0;
		int n2=1;
		int temp=n;
		System.out.print(n1+" "+n2+" ");
		for(int i=0;i<n-2;i++)
		{
			temp=n1+n2;
			System.out.print(temp+" ");
			n1=n2;
			n2=temp;
		}
		
	}
}
